<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.findmycoach.student.activity.UserListActivity">

    <include
        layout="@layout/header_back_name_menu"
        android:id="@+id/header" />
    
    <com.astuetz.PagerSlidingTabStrip
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:id="@+id/search_tabs"
        app:pstsIndicatorHeight="4dp"
        app:pstsIndicatorColor="@color/purple"
        app:pstsShouldExpand="true"
        android:layout_below="@+id/header"></com.astuetz.PagerSlidingTabStrip>


    <android.support.v4.view.ViewPager android:layout_below="@+id/search_tabs"
        android:id="@+id/search_view_pager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >


    </android.support.v4.view.ViewPager>

